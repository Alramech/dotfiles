URxvt*foreground:White
URxvt*background:Black
!URxvt*.transparent:True
! URxvt*.shading: 0 to 99 darkens, 101 to 200 lightens
!URxvt*.shading:10
! fonts
! run "fc-list" for a list of available fonts
URxvt*.depth:32
URxvt.background:[85]#000000


URxvt*font: xft:Tamsyn:pixelsize=16

URxvt*scrollBar: False
URxvt*scrollTtyOutput: False
URxvt*scrollTtyKeypress: True
URxvt*secondaryScroll: True
URxvt*saveLines: 8000

URxvt.perl-ext-common: default,matcher,readline,clipboard,tabbed

!URxvt.perl-ext-common : default,matcher
URxvt.urlLauncher     : /usr/bin/palemoon
URxvt.matcher.button  : 1
URxvt.matcher.rend.0: Uline Bold fg5

!URxvt.perl-ext-common : tabbed
URxvt.tabbed.tabbar-fg: 5
Urxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg:    14
URxvt.tabbed.tab-bg:    0

URxvt.iso14755: False

URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.clipboard.copycmd:  xsel -ib
URxvt.clipboard.pastecmd: xsel -ob
